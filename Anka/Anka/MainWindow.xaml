                        <!--#region 第六行-->
                        <Border BorderBrush="Black" BorderThickness="2,1" DockPanel.Dock="Top" Grid.Column="0" Grid.Row="5">
                            <StackPanel  HorizontalAlignment="Center" VerticalAlignment="Center" >
