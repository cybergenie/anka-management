<Window x:Name="window" x:Class="Anka.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Anka"
        mc:Ignorable="d"
        Title="术后康复进程" Height="797" Width="1000">
    <DockPanel>
        <Menu Height="25" DockPanel.Dock="Top" Margin="0" VerticalAlignment="Top">
            <MenuItem Header="文  件" Width="60" HorizontalContentAlignment="Center" Height="25">
                <MenuItem x:Name="DataConnection" Header="连接服务器" Click="DataConnection_Click"/>
                <MenuItem x:Name="miNewDoc" Header="新建档案" Click="MiNewDoc_Click"/>
                <Separator/>
                <MenuItem x:Name="miExit" Header="退出" Click="miExit_Click"/>
            </MenuItem>
            <MenuItem Header="选  项" Width="60"/>
        </Menu>
        <StatusBar Height="20" VerticalAlignment="Bottom" DockPanel.Dock="Bottom">
            <Button x:Name="ConStatus" Height="12" Width="50" IsEnabled="False"/>
        </StatusBar>
        <TabControl DockPanel.Dock="Right">
            <TabItem Header="基本信息">
                <ScrollViewer>
                    <StackPanel>
                        <StackPanel Orientation="Horizontal" Margin="5">
                            <Label Content="姓名：" Height="25" Width="45" />
                            <Label x:Name="lbBasicName" Content="" Height="25" Width="65" />
                            <Label Content="性别：" Height="25" Width="45" />
                            <Label x:Name="lbBasicMale" Content="" Height="25" Width="65" />
                            <Label Content="年龄：" Height="25" Width="45" />
                            <Label x:Name="lbBasicAge" Content="" Height="25" Width="65" />
                            <Label Content="病案号：" Height="25" Width="45" />
                            <Label x:Name="lbBasicNumber" Content="" Height="25" Width="80" />
                            <Button x:Name="btBasicSave" Content="保  存" HorizontalAlignment="Right" HorizontalContentAlignment="Center" Margin="85,0,0,0" Height="25" Width="45" Click="BtBasicSave_Click" />
                            <Button x:Name="btBasicLoad" Content="读  取" HorizontalAlignment="Right" HorizontalContentAlignment="Center" Margin="85,0,0,0" Height="25" Width="45" Click="BtBasicLoad_Click" />
                        </StackPanel>
                        <Grid DockPanel.Dock="Top" Margin="5">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="1"/>
                                <RowDefinition Height="70"/>
                                <RowDefinition Height="210"/>
                                <RowDefinition Height="70"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <Border BorderBrush="Black" Grid.ColumnSpan="4" BorderThickness="0,1">
                                <StackPanel HorizontalAlignment="Left" VerticalAlignment="Center" Orientation="Horizontal" Margin="5" >

                                </StackPanel>
                            </Border>
                            <StackPanel HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="5" Grid.Row="1">
                                <Label Content="心功能：" Height="25" Width="75" HorizontalAlignment="Left"/>
                                <StackPanel HorizontalAlignment="Left" VerticalAlignment="Bottom" Orientation="Horizontal" Grid.Column="3">
                                    <Label Content="Killip：" Height="25" Width="75" HorizontalAlignment="Left"/>
                                    <TextBox x:Name="txKillip" Height="25" TextWrapping="Wrap" VerticalContentAlignment="Center" Width="85"/>
                                </StackPanel>
                            </StackPanel>
                            <StackPanel HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="5" Grid.Row="1" Grid.Column="1" Orientation="Horizontal">
                                <Label Content="EF：" Height="25" Width="75" HorizontalAlignment="Left"/>
                                <TextBox x:Name="txEF" Height="25" TextWrapping="Wrap" Width="85" VerticalContentAlignment="Center"/>
                            </StackPanel>
                            <StackPanel HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="5" Grid.Row="1" Grid.Column="2" Orientation="Horizontal">
                                <Label Content="LV：" Height="25" Width="75" HorizontalAlignment="Left"/>
                                <TextBox x:Name="txLV" Height="25" TextWrapping="Wrap" Width="85" VerticalContentAlignment="Center"/>
                            </StackPanel>
                            <StackPanel HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="5" Grid.Row="1" Grid.Column="3" Orientation="Horizontal">
                                <Label Content="其  他：" Height="25" Width="75" VerticalContentAlignment="Center"/>
                                <TextBox x:Name="txBasicOther" Height="25" TextWrapping="Wrap" Width="85" VerticalContentAlignment="Center"/>
                            </StackPanel>

                            <StackPanel HorizontalAlignment="Left" VerticalAlignment="Center" Margin="5" Grid.Row="2" Grid.Column="0" Orientation="Horizontal">
                                <Label Content="危险因素：" Height="25" Width="75" VerticalContentAlignment="Center"/>
                            </StackPanel>

                            <StackPanel Margin="5,12,5,5" Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="2">
                                <StackPanel Orientation="Horizontal" VerticalAlignment="Center" >
                                    <CheckBox x:Name="cbRisk1" Content="高血压" Width="120" Height="25" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
                                    <CheckBox x:Name="cbRisk2" Content="糖尿病" Width="120" Height="25" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
                                    <CheckBox x:Name="cbRisk3" Content="脑卒中" Width="120" Height="25" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
                                </StackPanel>
                                <StackPanel VerticalAlignment="Center" Orientation="Horizontal" >
                                    <CheckBox x:Name="cbRisk4" Content="吸  烟" Height="25" Width="120" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" />
                                    <CheckBox x:Name="cbRisk5" Content="高LDL-C" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Width="120" Height="25" />
                                    <CheckBox x:Name="cbRisk6" Content="高  TG" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="120" Height="25" />
                                </StackPanel>
                                <StackPanel VerticalAlignment="Center" Orientation="Horizontal" >
                                    <CheckBox x:Name="cbRisk7" Content="肥  胖" Width="120" Height="25" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
                                    <CheckBox x:Name="cbRisk8" Content="痛  风" Width="120" Height="25" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
                                    <CheckBox x:Name="cbRisk9" Content="运动不足" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="120" Height="25" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" >
                                    <CheckBox x:Name="cbRisk10" Content="周围动脉硬化闭塞" Height="25" VerticalContentAlignment="Center" />
                                </StackPanel>
                                <StackPanel HorizontalAlignment="Left" VerticalAlignment="Center" Orientation="Horizontal" >
                                    <CheckBox x:Name="cbRisk11" Content="肾功能不全 CRE" VerticalContentAlignment="Center" Height="25" />
                                </StackPanel>
                                <StackPanel HorizontalAlignment="Left" VerticalAlignment="Center" Orientation="Horizontal" >
                                    <CheckBox x:Name="cbRisk12" Content="肝功能异常 ALT" Height="25" VerticalContentAlignment="Center" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" >
                                    <CheckBox x:Name="cbRisk13" Content="其他:" Height="25" Width="65" VerticalContentAlignment="Center" Click="CbRisk13_Click" />
                                    <TextBox x:Name="txRisk13" Width="135" Height="25" VerticalContentAlignment="Center" IsEnabled="False" />
                                </StackPanel>

                            </StackPanel>
                            <StackPanel HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="5" Grid.Row="3">
                                <Label Content="冠脉情况：" Height="25" Width="75" HorizontalAlignment="Left"/>
                                <StackPanel HorizontalAlignment="Left" VerticalAlignment="Bottom" Orientation="Horizontal">
                                    <Label Content="PCI支架数：" Height="25" HorizontalAlignment="Left"/>
                                    <TextBox x:Name="txPCI" Height="25" TextWrapping="Wrap" Width="50" VerticalContentAlignment="Center" HorizontalContentAlignment="Center"/>
                                    <Label Content="个" Height="25" Width="75" HorizontalAlignment="Left"/>
                                </StackPanel>
                            </StackPanel>
                            <StackPanel HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="5" Grid.Row="3" Grid.Column="1" Orientation="Horizontal">
                                <Label Content="75%以上残余狭窄" Height="25" HorizontalAlignment="Left"/>
                                <TextBox x:Name="txRS" Height="25" TextWrapping="Wrap" Width="50" VerticalContentAlignment="Center" HorizontalContentAlignment="Center"/>
                                <Label Content="个" Height="25" Width="75" HorizontalAlignment="Left"/>
                            </StackPanel>
                            <StackPanel HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="5" Grid.Row="3" Grid.Column="2" Orientation="Horizontal">
                                <CheckBox x:Name="cbCC" Content="侧枝循环：无" Height="25" VerticalContentAlignment="Center" Click="CbCC_Click" />
                            </StackPanel>
                            <StackPanel HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="0,5" Grid.Row="3" Grid.Column="3">
                                <Label Content="优势冠脉：" Height="25" Width="75" HorizontalAlignment="Left"/>
                                <StackPanel Orientation="Horizontal" Height="25">
                                    <RadioButton x:Name="rbDCR" Content="右优势型" VerticalContentAlignment="Center" />
                                    <RadioButton x:Name="rbDCB" Content="均衡型" VerticalContentAlignment="Center" />
                                    <RadioButton x:Name="rbDCL" Content="左优势型" VerticalContentAlignment="Center" />
                                </StackPanel>
                            </StackPanel>
                        </Grid>
                    </StackPanel>
                </ScrollViewer>
            </TabItem>
            <TabItem Header="运动负荷记录表">
                <ScrollViewer>
                    <StackPanel>
                        <StackPanel Orientation="Horizontal" Height="25" Margin="5">
                            <Label Content="姓名：" Height="25" Width="45" />
                            <Label x:Name="lbLoadName" Content="" Height="25" Width="65" />
                            <Label Content="性别：" Height="25" Width="45" />
                            <Label x:Name="lbLoadMale" Content="" Height="25" Width="65" />
                            <Label Content="年龄：" Height="25" Width="45" />
                            <Label x:Name="lbLoadAge" Content="" Height="25" Width="65" />
                            <Label Content="编号：" Height="25" Width="45" />
                            <Label x:Name="lbLoadNumber" Content="" Height="25" Width="80" />
                            <ComboBox x:Name="txExerciseLoop" Width="80" HorizontalAlignment="Left" Height="25" VerticalContentAlignment="Center" IsEditable="True" />
                            <Button x:Name="btExcerciseSave" Content="保  存" HorizontalAlignment="Right" HorizontalContentAlignment="Center" Margin="85,0,0,0" Width="45" Click="BtExcerciseSave_Click" />
                            <Button x:Name="btExcerciseLoad" Content="读  取" HorizontalAlignment="Right" HorizontalContentAlignment="Center" Margin="85,0,0,0" Height="25" Width="45" Click="BtExcerciseLoad_Click" />
                        </StackPanel>
                        <Grid DockPanel.Dock="Top" Margin="5" >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="35"/>
                                <RowDefinition Height="35"/>
                                <RowDefinition Height="35"/>
                                <RowDefinition Height="35"/>
                                <RowDefinition Height="55"/>
                                <RowDefinition Height="55"/>
                                <RowDefinition Height="55"/>
                                <RowDefinition Height="55"/>
                                <RowDefinition Height="55"/>
                                <RowDefinition Height="55"/>
                                <RowDefinition Height="25"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <!--#region 第一行-->
                            <Border BorderBrush="Black" BorderThickness="2,2,1,2" DockPanel.Dock="Top" Grid.RowSpan="4">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <Label Content="监测项目" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,2,1,0.5" DockPanel.Dock="Top" Grid.Column="1" Grid.ColumnSpan="4">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <Label Content="床上负荷(bed up)" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,2,1,0.5" DockPanel.Dock="Top" Grid.Column="5" Grid.ColumnSpan="2">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <Label Content="室内负荷" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,2,2,0.5" DockPanel.Dock="Top" Grid.Column="7" Grid.ColumnSpan="3">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <Label Content="室外负荷" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                </DockPanel>
                            </Border>
                            <!--#endregion-->
                            <!--#region 第二行-->
                            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" DockPanel.Dock="Top" Grid.Column="1" Grid.Row="1">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <Label Content="负荷前" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" DockPanel.Dock="Top" Grid.Column="2" Grid.Row="1">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <Label Content="被动" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" DockPanel.Dock="Top" Grid.Column="3" Grid.Row="1">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <Label Content="被动" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,1,0.5" DockPanel.Dock="Top" Grid.Column="4" Grid.Row="1">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <Label Content="被动/主动" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" DockPanel.Dock="Top" Grid.Column="5" Grid.Row="1">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <Label Content="负荷前" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,1,0.5" DockPanel.Dock="Top" Grid.Column="6" Grid.Row="1">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <Label Content="坐起" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" DockPanel.Dock="Top" Grid.Column="7" Grid.Row="1">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <Label Content="负荷前" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" DockPanel.Dock="Top" Grid.Column="8" Grid.Row="1">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <Label Content="步行" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,2,0.5" DockPanel.Dock="Top" Grid.Column="9" Grid.Row="1">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <Label Content="踏车" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                </DockPanel>
                            </Border>
                            <!--#endregion-->
                            <!--#region 第三行 -->
                            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" DockPanel.Dock="Top" Grid.Column="1" Grid.Row="2">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <Label Content="俯仰位" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" DockPanel.Dock="Top" Grid.Column="2" Grid.Row="2">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <Label Content="30度" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" DockPanel.Dock="Top" Grid.Column="3" Grid.Row="2">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <Label Content="60度" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,1,0.5" DockPanel.Dock="Top" Grid.Column="4" Grid.Row="2">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <Label Content="90度" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" DockPanel.Dock="Top" Grid.Column="5" Grid.Row="2">
                                <StackPanel  HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <Label Content="端坐位" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                </StackPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,1,0.5" DockPanel.Dock="Top" Grid.Column="6" Grid.Row="2">
                                <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Horizontal" >
                                    <RadioButton x:Name="rbInRoomUp5" Content="5" Margin="5,0" />
                                    <RadioButton x:Name="rbInRoomUp10" Content="10" />
                                </StackPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" DockPanel.Dock="Top" Grid.Column="7" Grid.Row="2">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <Label Content="立或坐位" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" DockPanel.Dock="Top" Grid.Column="8" Grid.Row="2">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <Label Content="2M" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,2,0.5" DockPanel.Dock="Top" Grid.Column="9" Grid.Row="2">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <Label Content="10W*5M" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                </DockPanel>
                            </Border>
                            <!--#endregion-->
                            <!--#region 第四行 -->
                            <Border BorderBrush="Black" BorderThickness="1,1,0.5,2" DockPanel.Dock="Top" Grid.Column="1" Grid.Row="3">
                                <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" >
                                    <DatePicker x:Name="dpBedup1" ToolTip="{Binding Text, RelativeSource={RelativeSource Self}}" />
                                </StackPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,0.5,2" DockPanel.Dock="Top" Grid.Column="2" Grid.Row="3">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <DatePicker x:Name="dpBedup2" ToolTip="{Binding Text, RelativeSource={RelativeSource Self}}" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,0.5,2" DockPanel.Dock="Top" Grid.Column="3" Grid.Row="3">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <DatePicker x:Name="dpBedup3" ToolTip="{Binding Text, RelativeSource={RelativeSource Self}}" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,1,2" DockPanel.Dock="Top" Grid.Column="4" Grid.Row="3">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <DatePicker x:Name="dpBedup4" ToolTip="{Binding Text, RelativeSource={RelativeSource Self}}" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,0.5,2" DockPanel.Dock="Top" Grid.Column="5" Grid.Row="3">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <DatePicker x:Name="dpInRoom1" ToolTip="{Binding Text, RelativeSource={RelativeSource Self}}" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,1,2" DockPanel.Dock="Top" Grid.Column="6" Grid.Row="3">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <DatePicker x:Name="dpInRoom2" ToolTip="{Binding Text, RelativeSource={RelativeSource Self}}" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,0.5,2" DockPanel.Dock="Top" Grid.Column="7" Grid.Row="3">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <DatePicker x:Name="dpOutRoom1" ToolTip="{Binding Text, RelativeSource={RelativeSource Self}}" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,0.5,2" DockPanel.Dock="Top" Grid.Column="8" Grid.Row="3">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <DatePicker x:Name="dpOutRoom2" ToolTip="{Binding Text, RelativeSource={RelativeSource Self}}" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,2,2" DockPanel.Dock="Top" Grid.Column="9" Grid.Row="3">
                                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <DatePicker x:Name="dpOutRoom3" ToolTip="{Binding Text, RelativeSource={RelativeSource Self}}" />
                                </DockPanel>
                            </Border>
                            <!--#endregion-->

                            <!--#region 第五行-->
                            <Border BorderBrush="Black" BorderThickness="2,1" DockPanel.Dock="Top" Grid.Column="0" Grid.Row="4">
                                <StackPanel  HorizontalAlignment="Center" VerticalAlignment="Center" >
                                    <Label Content="血压" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                    <Label Content="(mmHg)" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                </StackPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,0.5,1" DockPanel.Dock="Top" Grid.Column="1" Grid.Row="4">
                                <DockPanel >
                                    <TextBox x:Name="txBPBedUp1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Text="/" GotFocus="TxBloodPressure_GotFocus" LostFocus="TxBloodPressure_LostFocus" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,0.5,1" DockPanel.Dock="Top" Grid.Column="2" Grid.Row="4">
                                <DockPanel >
                                    <TextBox x:Name="txBPBedUp2" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Text="/" GotFocus="TxBloodPressure_GotFocus" LostFocus="TxBloodPressure_LostFocus" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,0.5,1" DockPanel.Dock="Top" Grid.Column="3" Grid.Row="4">
                                <DockPanel >
                                    <TextBox x:Name="txBPBedUp3" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Text="/" GotFocus="TxBloodPressure_GotFocus" LostFocus="TxBloodPressure_LostFocus" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1" DockPanel.Dock="Top" Grid.Column="4" Grid.Row="4">
                                <DockPanel >
                                    <TextBox x:Name="txBPBedUp4" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Text="/" GotFocus="TxBloodPressure_GotFocus" LostFocus="TxBloodPressure_LostFocus" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1,1,0.5,1" DockPanel.Dock="Top" Grid.Column="5" Grid.Row="4">
                                <DockPanel >
                                    <TextBox x:Name="txBPInRoom1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Text="/" GotFocus="TxBloodPressure_GotFocus" LostFocus="TxBloodPressure_LostFocus" />
                                </DockPanel>
                            </Border>
                            <Border BorderBrush="Black" BorderThickness="1" DockPanel.Dock="Top" Grid.Column="6" Grid.Row="4">
                                <DockPanel >
                                    <TextBox x:Name="txBPInRoom2" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Text="/" GotFocus="TxBloodPressure_GotFocus" LostFocus="TxBloodPressure_LostFocus" />
                                </DockPanel>
