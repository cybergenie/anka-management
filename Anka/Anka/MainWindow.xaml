<Window x:Class="Anka.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Anka"
        mc:Ignorable="d"
        Title="MainWindow" Height="750" Width="1000">
    <DockPanel>
        <Menu Height="20" DockPanel.Dock="Top" Margin="0" VerticalAlignment="Top">
            <Button Content="12"/>
            <Button Content="22"/>
            <Button/>
            <Button/>
        </Menu>
        <StatusBar Height="20" VerticalAlignment="Bottom" DockPanel.Dock="Bottom"/>
        <TabControl DockPanel.Dock="Right">
            <TabItem Header="基本信息">
                <StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <Label Content="姓名：" Height="25" Width="85" />
                        <Label Content="性别：" Height="25" Width="85" />
                        <Label Content="年龄：" Height="25" Width="85" />
                        <Label Content="编号：" Height="25" Width="120" />
                        <TextBox Width="45" HorizontalAlignment="Left" Height="25" />
                        <Button Content="确定" HorizontalAlignment="Right" HorizontalContentAlignment="Center" Margin="85,0,0,0" Height="25" Width="45" />
                    </StackPanel>
                    <Grid DockPanel.Dock="Top" Margin="5">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1"/>
                            <RowDefinition Height="70"/>
                            <RowDefinition Height="210"/>
                            <RowDefinition Height="70"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <Border BorderBrush="Black" Grid.ColumnSpan="4" BorderThickness="0,1">
                            <StackPanel HorizontalAlignment="Left" VerticalAlignment="Center" Orientation="Horizontal" Margin="5" >

                            </StackPanel>
                        </Border>
                        <StackPanel HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="5" Grid.Row="1">
                            <Label Content="心功能：" Height="25" Width="75" HorizontalAlignment="Left"/>
                            <StackPanel HorizontalAlignment="Left" VerticalAlignment="Bottom" Orientation="Horizontal" Grid.Column="3">
                                <Label Content="Killip：" Height="25" Width="75" HorizontalAlignment="Left"/>
                                <TextBox Height="25" TextWrapping="Wrap" Text="TextBox" VerticalContentAlignment="Center" Width="85"/>
                            </StackPanel>
                        </StackPanel>
                        <StackPanel HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="5" Grid.Row="1" Grid.Column="1" Orientation="Horizontal">
                            <Label Content="EF：" Height="25" Width="75" HorizontalAlignment="Left"/>
                            <TextBox Height="25" TextWrapping="Wrap" Text="TextBox" Width="85" VerticalContentAlignment="Center"/>
                        </StackPanel>
                        <StackPanel HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="5" Grid.Row="1" Grid.Column="2" Orientation="Horizontal">
                            <Label Content="LV：" Height="25" Width="75" HorizontalAlignment="Left"/>
                            <TextBox Height="25" TextWrapping="Wrap" Text="TextBox" Width="85" VerticalContentAlignment="Center"/>
                        </StackPanel>
                        <StackPanel HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="5" Grid.Row="1" Grid.Column="3" Orientation="Horizontal">
                            <Label Content="其  他：" Height="25" Width="75" VerticalContentAlignment="Center"/>
                            <TextBox Height="25" TextWrapping="Wrap" Text="TextBox" Width="85" VerticalContentAlignment="Center"/>
                        </StackPanel>

                        <StackPanel HorizontalAlignment="Left" VerticalAlignment="Center" Margin="5" Grid.Row="2" Grid.Column="0" Orientation="Horizontal">
                            <Label Content="危险因素：" Height="25" Width="75" VerticalContentAlignment="Center"/>
                        </StackPanel>

                        <StackPanel Margin="5,12,5,5" Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="2">
                            <StackPanel Orientation="Horizontal" VerticalAlignment="Center" >
                                <CheckBox Content="高血压" Width="120" Height="25" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
                                <CheckBox Content="糖尿病" Width="120" Height="25" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
                                <CheckBox Content="脑卒中" Width="120" Height="25" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
                            </StackPanel>
                            <StackPanel VerticalAlignment="Center" Orientation="Horizontal" >
                                <CheckBox Content="吸  烟" Height="25" Width="120" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" />
                                <CheckBox Content="高LDL-C" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Width="120" Height="25" />
                                <CheckBox Content="高  TG" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="120" Height="25" />
                            </StackPanel>
                            <StackPanel VerticalAlignment="Center" Orientation="Horizontal" >
                                <CheckBox Content="肥  胖" Width="120" Height="25" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
                                <CheckBox Content="痛  风" Width="120" Height="25" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
                                <CheckBox Content="运动不足" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="120" Height="25" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" >
                                <CheckBox Content="周围动脉硬化闭塞" Height="25" VerticalContentAlignment="Center" />
                            </StackPanel>
                            <StackPanel HorizontalAlignment="Left" VerticalAlignment="Center" Orientation="Horizontal" >
                                <CheckBox Content="肾功能不全 CRE" VerticalContentAlignment="Center" Height="25" />
                            </StackPanel>
                            <StackPanel HorizontalAlignment="Left" VerticalAlignment="Center" Orientation="Horizontal" >
                                <CheckBox Content="肝功能异常 ALT" Height="25" VerticalContentAlignment="Center" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" >
                                <CheckBox Content="其他:" Height="25" Width="65" VerticalContentAlignment="Center" />
                                <TextBox Width="135" Height="25" VerticalContentAlignment="Center" />
                            </StackPanel>

                        </StackPanel>
                        <StackPanel HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="5" Grid.Row="3">
                            <Label Content="冠脉情况：" Height="25" Width="75" HorizontalAlignment="Left"/>
                            <StackPanel HorizontalAlignment="Left" VerticalAlignment="Bottom" Orientation="Horizontal">
                                <Label Content="PCI支架数：" Height="25" HorizontalAlignment="Left"/>
                                <TextBox Height="25" TextWrapping="Wrap" Width="50" VerticalContentAlignment="Center" HorizontalContentAlignment="Center"/>
                                <Label Content="个" Height="25" Width="75" HorizontalAlignment="Left"/>
                            </StackPanel>
                        </StackPanel>
                        <StackPanel HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="5" Grid.Row="3" Grid.Column="1" Orientation="Horizontal">
                            <Label Content="75%以上残余狭窄" Height="25" HorizontalAlignment="Left"/>
                            <TextBox Height="25" TextWrapping="Wrap" Width="50" VerticalContentAlignment="Center" HorizontalContentAlignment="Center"/>
                            <Label Content="个" Height="25" Width="75" HorizontalAlignment="Left"/>
                        </StackPanel>
                        <StackPanel HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="5" Grid.Row="3" Grid.Column="2" Orientation="Horizontal">
                            <CheckBox Content="侧枝循环：无" Height="25" VerticalContentAlignment="Center" />
                        </StackPanel>
                        <StackPanel HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="0,5" Grid.Row="3" Grid.Column="3">
                            <Label Content="优势冠脉：" Height="25" Width="75" HorizontalAlignment="Left"/>
                            <StackPanel Orientation="Horizontal" Height="25">
                                <RadioButton Content="右优势型" VerticalContentAlignment="Center" />
                                <RadioButton Content="均衡型" VerticalContentAlignment="Center" />
                                <RadioButton Content="左优势型" VerticalContentAlignment="Center" />
                            </StackPanel>
                        </StackPanel>
                    </Grid>
                </StackPanel>
            </TabItem>
            <TabItem Header="运动负荷记录表">
                <StackPanel>
                    <StackPanel Orientation="Horizontal" Height="25" Margin="5">
                        <Label Content="姓名：" Height="25" Width="85" />
                        <Label Content="性别：" Height="25" Width="85" />
                        <Label Content="年龄：" Height="25" Width="85" />
                        <Label Content="编号：" Height="25" Width="120" />
                        <TextBox Width="45" HorizontalAlignment="Left" Height="25" />
                        <Button Content="确定" HorizontalAlignment="Right" HorizontalContentAlignment="Center" Margin="85,0,0,0" Height="25" Width="45" />
                    </StackPanel>
                    <Grid DockPanel.Dock="Top" Margin="5" >
