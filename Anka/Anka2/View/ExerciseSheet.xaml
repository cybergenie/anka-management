                <DockPanel >
                    <TextBox x:Name="txBOBedUp4" HorizontalContentAlignment="Center"  Text="" />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,0.5,1" DockPanel.Dock="Top" Grid.Column="5" Grid.Row="6">
                <DockPanel >
                    <TextBox x:Name="txBOInRoom1" HorizontalContentAlignment="Center"  Text="" />
                </DockPanel>
