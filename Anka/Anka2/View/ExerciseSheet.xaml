            <Border BorderBrush="Black" BorderThickness="1" DockPanel.Dock="Top" Grid.Column="6" Grid.Row="6">
                <DockPanel >
                    <TextBox x:Name="txBOInRoom2" HorizontalContentAlignment="Center"  Text="" />
