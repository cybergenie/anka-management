                <StackPanel  HorizontalAlignment="Center" VerticalAlignment="Center" >
                    <Label Content="血氧饱和度" VerticalAlignment="Center" HorizontalAlignment="Center" />
                    <Label Content="(%)" VerticalAlignment="Center" HorizontalAlignment="Center" />
                </StackPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,0.5,1" DockPanel.Dock="Top" Grid.Column="1" Grid.Row="6">
