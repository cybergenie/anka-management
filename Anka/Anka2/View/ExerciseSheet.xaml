<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <StackPanel x:Key="Exercise">
        <StackPanel Orientation="Horizontal" Height="25" Margin="5">
            <Label Content="姓名：" Height="25" Width="45" />
            <Label x:Name="lbLoadName" Content="" Height="25" Width="65" />
            <Label Content="性别：" Height="25" Width="45" />
            <Label x:Name="lbLoadMale" Content="" Height="25" Width="65" />
            <Label Content="年龄：" Height="25" Width="45" />
            <Label x:Name="lbLoadAge" Content="" Height="25" Width="65" />
            <Label Content="编号：" Height="25" Width="45" />
            <Label x:Name="lbLoadNumber" Content="" Height="25" Width="80" />
            <ComboBox x:Name="txExerciseLoop" Width="80" HorizontalAlignment="Left" Height="25" VerticalContentAlignment="Center" IsEditable="True" />
            <Button x:Name="btExcerciseSave" Content="保  存" HorizontalAlignment="Right" HorizontalContentAlignment="Center" Margin="85,0,0,0" Width="45"  />
            <Button x:Name="btExcerciseLoad" Content="读  取" HorizontalAlignment="Right" HorizontalContentAlignment="Center" Margin="85,0,0,0" Height="25" Width="45"/>
        </StackPanel>
        <Grid DockPanel.Dock="Top" Margin="5" >
            <Grid.RowDefinitions>
                <RowDefinition Height="35"/>
                <RowDefinition Height="35"/>
                <RowDefinition Height="35"/>
                <RowDefinition Height="35"/>
                <RowDefinition Height="55"/>
                <RowDefinition Height="55"/>
                <RowDefinition Height="55"/>
                <RowDefinition Height="55"/>
                <RowDefinition Height="55"/>
                <RowDefinition Height="55"/>
                <RowDefinition Height="25"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <!--#region 第一行-->
            <Border BorderBrush="Gray" BorderThickness="2,2,1,2" DockPanel.Dock="Top" Grid.RowSpan="4">
                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                    <Label Content="监测项目" VerticalAlignment="Center" HorizontalAlignment="Center" />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,2,1,0.5" DockPanel.Dock="Top" Grid.Column="1" Grid.ColumnSpan="4">
                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                    <Label Content="床上负荷(bed up)" VerticalAlignment="Center" HorizontalAlignment="Center" />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,2,1,0.5" DockPanel.Dock="Top" Grid.Column="5" Grid.ColumnSpan="2">
                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                    <Label Content="室内负荷" VerticalAlignment="Center" HorizontalAlignment="Center" />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,2,2,0.5" DockPanel.Dock="Top" Grid.Column="7" Grid.ColumnSpan="3">
                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                    <Label Content="室外负荷" VerticalAlignment="Center" HorizontalAlignment="Center" />
                </DockPanel>
            </Border>
            <!--#endregion-->
            <!--#region 第二行-->
            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" DockPanel.Dock="Top" Grid.Column="1" Grid.Row="1">
                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                    <Label Content="负荷前" VerticalAlignment="Center" HorizontalAlignment="Center" />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" DockPanel.Dock="Top" Grid.Column="2" Grid.Row="1">
                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                    <Label Content="被动" VerticalAlignment="Center" HorizontalAlignment="Center" />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" DockPanel.Dock="Top" Grid.Column="3" Grid.Row="1">
                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                    <Label Content="被动" VerticalAlignment="Center" HorizontalAlignment="Center" />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,1,0.5" DockPanel.Dock="Top" Grid.Column="4" Grid.Row="1">
                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                    <Label Content="被动/主动" VerticalAlignment="Center" HorizontalAlignment="Center" />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" DockPanel.Dock="Top" Grid.Column="5" Grid.Row="1">
                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                    <Label Content="负荷前" VerticalAlignment="Center" HorizontalAlignment="Center" />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,1,0.5" DockPanel.Dock="Top" Grid.Column="6" Grid.Row="1">
                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                    <Label Content="坐起" VerticalAlignment="Center" HorizontalAlignment="Center" />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" DockPanel.Dock="Top" Grid.Column="7" Grid.Row="1">
                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                    <Label Content="负荷前" VerticalAlignment="Center" HorizontalAlignment="Center" />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" DockPanel.Dock="Top" Grid.Column="8" Grid.Row="1">
                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                    <Label Content="步行" VerticalAlignment="Center" HorizontalAlignment="Center" />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,2,0.5" DockPanel.Dock="Top" Grid.Column="9" Grid.Row="1">
                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                    <Label Content="踏车" VerticalAlignment="Center" HorizontalAlignment="Center" />
                </DockPanel>
            </Border>
            <!--#endregion-->
            <!--#region 第三行 -->
            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" DockPanel.Dock="Top" Grid.Column="1" Grid.Row="2">
                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                    <Label Content="俯仰位" VerticalAlignment="Center" HorizontalAlignment="Center" />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" DockPanel.Dock="Top" Grid.Column="2" Grid.Row="2">
                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                    <Label Content="30度" VerticalAlignment="Center" HorizontalAlignment="Center" />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" DockPanel.Dock="Top" Grid.Column="3" Grid.Row="2">
                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                    <Label Content="60度" VerticalAlignment="Center" HorizontalAlignment="Center" />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,1,0.5" DockPanel.Dock="Top" Grid.Column="4" Grid.Row="2">
                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                    <Label Content="90度" VerticalAlignment="Center" HorizontalAlignment="Center" />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" DockPanel.Dock="Top" Grid.Column="5" Grid.Row="2">
                <StackPanel  HorizontalAlignment="Center" VerticalAlignment="Center" >
                    <Label Content="端坐位" VerticalAlignment="Center" HorizontalAlignment="Center" />
                </StackPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,1,0.5" DockPanel.Dock="Top" Grid.Column="6" Grid.Row="2">
                <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Horizontal" >
                    <RadioButton x:Name="rbInRoomUp5" Content="5" Margin="5,0" />
                    <RadioButton x:Name="rbInRoomUp10" Content="10" />
                </StackPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" DockPanel.Dock="Top" Grid.Column="7" Grid.Row="2">
                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                    <Label Content="立或坐位" VerticalAlignment="Center" HorizontalAlignment="Center" />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" DockPanel.Dock="Top" Grid.Column="8" Grid.Row="2">
                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                    <Label Content="2M" VerticalAlignment="Center" HorizontalAlignment="Center" />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,2,0.5" DockPanel.Dock="Top" Grid.Column="9" Grid.Row="2">
                <DockPanel LastChildFill="False" HorizontalAlignment="Center" VerticalAlignment="Center" >
                    <Label Content="10W*5M" VerticalAlignment="Center" HorizontalAlignment="Center" />
                </DockPanel>
            </Border>
            <!--#endregion-->
            <!--#region 第四行 -->
            <Border BorderBrush="Black" BorderThickness="1,1,0.5,2" DockPanel.Dock="Top" Grid.Column="1" Grid.Row="3">
                <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" >
                    <DatePicker x:Name="dpBedup1" ToolTip="{Binding Text, RelativeSource={RelativeSource Self}}" />
                </StackPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,0.5,2" DockPanel.Dock="Top" Grid.Column="2" Grid.Row="3">
