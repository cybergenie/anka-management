            <Border BorderBrush="Black" BorderThickness="1,1,0.5,1" DockPanel.Dock="Top" Grid.Column="1" Grid.Row="7">
                <DockPanel >
                    <TextBox x:Name="txBorgBedUp1" HorizontalContentAlignment="Center" Text="" />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,0.5,1" DockPanel.Dock="Top" Grid.Column="2" Grid.Row="7">
                <DockPanel >
                    <TextBox x:Name="txBorgBedUp2" HorizontalContentAlignment="Center" Text="" />
                </DockPanel>
