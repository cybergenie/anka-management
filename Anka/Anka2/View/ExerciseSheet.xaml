            <Border BorderBrush="Black" BorderThickness="1,1,0.5,1" DockPanel.Dock="Top" Grid.Column="5" Grid.Row="5">
                <DockPanel >
                    <TextBox x:Name="txBMPInRoom1" HorizontalContentAlignment="Center"  Text="" />
