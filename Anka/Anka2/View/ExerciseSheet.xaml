            <!--#region 第五行-->
            <Border BorderBrush="Black" BorderThickness="2,1" DockPanel.Dock="Top" Grid.Column="0" Grid.Row="4">
                <StackPanel  HorizontalAlignment="Center" VerticalAlignment="Center" >
                    <Label Content="血压" VerticalAlignment="Center" HorizontalAlignment="Center" />
                    <Label Content="(mmHg)" VerticalAlignment="Center" HorizontalAlignment="Center" />
                </StackPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,0.5,1" DockPanel.Dock="Top" Grid.Column="1" Grid.Row="4">
                <DockPanel >
                    <TextBox x:Name="txBPBedUp1" HorizontalContentAlignment="Center" Text="/"  />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,0.5,1" DockPanel.Dock="Top" Grid.Column="2" Grid.Row="4">
                <DockPanel >
                    <TextBox x:Name="txBPBedUp2" HorizontalContentAlignment="Center"  Text="/"  />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,0.5,1" DockPanel.Dock="Top" Grid.Column="3" Grid.Row="4">
                <DockPanel >
                    <TextBox x:Name="txBPBedUp3" HorizontalContentAlignment="Center"  Text="/" />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1" DockPanel.Dock="Top" Grid.Column="4" Grid.Row="4">
                <DockPanel >
                    <TextBox x:Name="txBPBedUp4" HorizontalContentAlignment="Center" Text="/"  />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,0.5,1" DockPanel.Dock="Top" Grid.Column="5" Grid.Row="4">
                <DockPanel >
                    <TextBox x:Name="txBPInRoom1" HorizontalContentAlignment="Center"  Text="/" />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1" DockPanel.Dock="Top" Grid.Column="6" Grid.Row="4">
                <DockPanel >
                    <TextBox x:Name="txBPInRoom2" HorizontalContentAlignment="Center" Text="/" />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,0.5,1" DockPanel.Dock="Top" Grid.Column="7" Grid.Row="4">
                <DockPanel >
                    <TextBox x:Name="txBPOutRoom1" HorizontalContentAlignment="Center"  Text="/"  />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,0.5,1" DockPanel.Dock="Top" Grid.Column="8" Grid.Row="4">
                <DockPanel >
                    <TextBox x:Name="txBPOutRoom2" HorizontalContentAlignment="Center" Text="/" />
                </DockPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1,1,2,1" DockPanel.Dock="Top" Grid.Column="9" Grid.Row="4">
                <DockPanel >
                    <TextBox x:Name="txBPOutRoom3" HorizontalContentAlignment="Center" Text="/" />
                </DockPanel>
            </Border>
            <!--#endregion-->
            <!--#region 第六行-->
            <Border BorderBrush="Black" BorderThickness="2,1" DockPanel.Dock="Top" Grid.Column="0" Grid.Row="5">
                <StackPanel  HorizontalAlignment="Center" VerticalAlignment="Center" >
