<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Anka2.CustomControls">


    <Style TargetType="{x:Type local:ValueBar}">     

        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:ValueBar}">
                    <Canvas Width="120" Height="50" >
                        <Rectangle
                            Width="150"
                            Height="50"                            
                            Stroke="Gray" StrokeThickness="0.5"
                            RadiusX="25" RadiusY="25"
                            Canvas.Left="0"
                            Canvas.Top="0"/>
                        <Rectangle
                            Width="60"
                            Height="50"
                            Fill="Red"
                            Stroke="Gray" StrokeThickness="0.5"                           
                            Canvas.Left="40"
                            Canvas.Top="0"/>
                    </Canvas>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>
